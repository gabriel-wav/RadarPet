# Endereço do seu servidor (pode ser 'localhost' ou um nome como 'NOME-PC\SQLEXPRESS')
DB_SERVER=localhost
# Porta do servidor (padrão do postgres é 5432)
DB_PORT=5432
# Nome do banco de dados que será criado
DB_DATABASE=radar_pet
# Deixe as credenciais vazias
DB_USERNAME=postgres
DB_PASSWORD=9090
# Mude para 'true' para usar a autenticação do Windows
USE_WINDOWS_AUTH=true